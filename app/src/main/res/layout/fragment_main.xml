            android:background="#00FFE9E9" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="TextView"
            android:layout_width="145dp"
            android:layout_height="46dp"
            android:id="@+id/main_textview" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="648dp"
            app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView" app:layout_constraintVertical_bias="0.0"
    />
</androidx.constraintlayout.widget.ConstraintLayout>